{
    "name": "sal",
    "displayName": "SALL lang",
    "description": "language support for SALL",
    "version": "0.0.1",
    "publisher": "abromage",
    "icon": "images/sall-icon.png",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "sal",
            "aliases": ["SALL", "sal"],
            "extensions": [".sal"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "sal",
            "scopeName": "source.sal",
            "path": "./syntaxes/sal.tmLanguage.json"
        }],
        "snippets": [{
            "language": "sal",
            "path": "./snippets/sal.json"
        },{
            "language": "sal",
            "path": "./snippets/sall-ergon-snippets.json"
        }]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Zix44/vscode-sall-lang.git"
    }
}